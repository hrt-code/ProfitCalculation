{
  "scripts": {
    "tailwind:watch": "npx tailwindcss -i ./src/assets/sass/style.scss -o ./build/css/main.css --watch",
    "serve": "live-server ./src",
    "sass:watch": "sass ./src/assets/sass/style.scss ./build/css/main.css --watch ",
    "tsc:watch": "tsc -w",
    "dev": "concurrently --kill-others \"npm run tailwind:watch\" \"npm run sass:watch\" \"npm run tsc:watch\""
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "tailwindcss": "^3.0.24",
    "type": "module"
  },
  "dependencies": {
    "live-server": "^1.2.1",
    "rxjs": "^7.5.5",
    "sass": "^1.50.0"
  },
