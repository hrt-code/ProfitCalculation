{
  "name": "profit",
  "version": "1.0.0",
  "description": "profit calculation",
  "main": "index.js",
  "scripts": {
    "tailwind:watch": "npx tailwindcss -i ./src/assets/sass/style.scss -o ./build/css/main.css --watch",
    "serve": "live-server ./src",
    "sass:watch": "sass ./src/assets/sass/style.scss ./build/css/main.css --watch ",
    "tsc:watch": "tsc -w",
    "webpack:watch":"webpack --watch",
    "dev": "concurrently --kill-others \"npm run tailwind:watch\" \"npm run sass:watch\" \"npm run webpack:watch\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.1.0",
    "rxjs": "^7.5.5",
    "sass": "^1.50.0"
  },
  "devDependencies": {
    "tailwindcss": "^3.0.24",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
